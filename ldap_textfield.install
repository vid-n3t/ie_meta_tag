<?php
/**
 * @file
 * Install, update and uninstall functions for the ldap_textfield module.
 */

/**
 * Implements hook_uninstall().
 *
 * @ingroup ldap_textfield
 */
function ldap_textfield_uninstall() {
  variable_del('ldap_textfield_enabled');
  variable_del('ldap_textfield_debug_message');
  variable_del('ldap_textfield_debug_reporting');
}
